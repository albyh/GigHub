@{
    ViewBag.Title = "Home Page";
}

<div class="jumbotron">
    <h1>ASP.NET</h1>
    <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
    <p><a href="http://asp.net" class="btn btn-primary btn-lg">Learn more &raquo;</a></p>
</div>

<div class="row">
    <div class="col-md-4">
        <h2>Register</h2>
        <p>Register for an account.</p>
        @*<p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301865">Register &raquo;</a>*@
        <p>@Html.ActionLink("Register","Register", "Account", null, new {@class="btn btn-default"} )</p>
        </p>
    </div>
    <div class="col-md-4">
        <h2>Log in</h2>
        <p>Access your existing account.</p>
        @*<p><a class="btn btn-default" href="http://go.microsoft.com/fwlink/?LinkId=301866">Log In &raquo;</a>*@
        <p>@Html.ActionLink("Log in", "Login", "Account", null, new { @class = "btn btn-default" })</p>
        </p>
    </div>
    <div class="col-md-4">
        <h2>Forgot Password</h2>
        <p>Forgot your password?</p>
        <p>@Html.ActionLink("Forgot PW", "ForgotPassword", "Account", null, new { @class = "btn btn-default" })</p>
        </p>
    </div>

</div>
@section scripts {
    <script>var testing = true</script>
}

